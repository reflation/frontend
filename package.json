    "typescript": "3.6.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@types/hapi": "^18.0.2",
    "@types/jest": "24.0.18",
    "@types/node": "^12.7.5",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/styled-components": "^4.1.19",
    "cross-env": "^6.0.0",
    "eslint": "^6.4.0",
